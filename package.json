{
  "name": "angular-webshop",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server",
    "build": "npm run build:client",
    "build:all": "npm run clean:all && npm run build:client & npm run build:server",
    "build:all:aot": "npm run clean:all && npm run build:client:aot & npm run build:server:aot",
    "build:client": "./node_modules/webpack/bin/webpack.js --env.client -d",
    "build:client:aot": "./node_modules/webpack/bin/webpack.js --env.aot --env.client",
    "build:server": "./node_modules/webpack/bin/webpack.js --env.server -d && npm run clean:build",
    "build:server:aot": "./node_modules/webpack/bin/webpack.js --env.aot --env.server && npm run clean:build",
    "watch:all": "npm run clean:all && npm run watch:client & npm run watch:server",
    "watch:client": "./node_modules/webpack/bin/webpack.js --env.client -d --watch",
    "watch:server": "./node_modules/webpack/bin/webpack.js --env.server -d --watch",
    "clean:all": "rimraf dist && rimraf src/aot",
    "clean:build": "rimraf dist/out-tsc && rimraf src/aot",
    "clean:dist": "rimraf dist",
    "server": "nodemon dist/main.server.bundle.js",
    "sw": "sw-precache --config=sw-precache-config.js",
    "test": "karma start ./karma.conf.js",
    "lint": "tslint src/**/*.ts",
    "lint-staged": "lint-staged",
    "stylelint-staged": "stylelint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.1.1",
    "@angular/common": "4.1.1",
    "@angular/compiler": "4.1.1",
    "@angular/core": "4.1.1",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.1",
    "@angular/platform-browser": "4.1.1",
    "@angular/platform-browser-dynamic": "4.1.1",
    "@angular/platform-server": "4.1.1",
    "@angular/router": "4.1.1",
    "angular2-jwt": "0.2.3",
    "base64-img": "1.0.3",
    "body-parser": "1.17.1",
    "compression": "1.6.2",
    "core-js": "2.4.1",
    "credential": "2.0.0",
    "express": "4.15.2",
    "fastclick": "1.0.6",
    "jsonwebtoken": "7.4.0",
    "mongodb": "2.2.26",
    "ng2-img-cropper": "0.8.8",
    "qrcode-generator": "1.2.0",
    "qrcodejs2": "0.0.2",
    "ric": "1.3.0",
    "rxjs": "5.4.0",
    "speakeasy": "2.0.0",
    "sweetalert2": "6.6.2",
    "web-animations-js": "2.2.5",
    "zone.js": "0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "1.1.0-beta.1",
    "@angular/compiler-cli": "4.1.1",
    "@angular/language-service": "4.1.1",
    "@types/express": "4.0.35",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.18",
    "@types/speakeasy": "2.0.1",
    "@types/webpack": "2.2.15",
    "autoprefixer": "7.0.1",
    "codelyzer": "3.0.1",
    "css-loader": "0.28.1",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "4.1.0",
    "json-loader": "0.5.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.3",
    "lint-staged": "3.4.1",
    "nodemon": "1.11.0",
    "postcss-loader": "2.0.5",
    "postcss-url": "6.0.4",
    "pre-commit": "1.2.2",
    "protractor": "5.1.1",
    "pug": "2.0.0-rc.1",
    "pug-html-loader": "1.1.4",
    "pug-lint": "2.4.0",
    "purifycss-webpack": "0.6.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "script-ext-html-webpack-plugin": "1.7.1",
    "script-loader": "0.7.0",
    "source-map-loader": "0.2.1",
    "style-loader": "0.17.0",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "stylint": "1.5.9",
    "stylus-loader": "3.0.1",
    "sw-precache": "5.1.1",
    "ts-node": "3.0.4",
    "tslint": "4.5.1",
    "typescript": "2.3.2",
    "url-loader": "0.5.8",
    "webpack-dev-server": "2.4.5"
  },
  "greenkeeper": {
    "branchPrefix": "dep-update-",
    "label": "dep-update",
    "ignore": [
      "tslint"
    ]
  },
  "lint-staged": {
    "*.ts": "tslint",
    "*.css": "stylelint",
    "*.pug": "pug-lint",
    "*.styl": "stylint"
  },
  "pre-commit": "lint-staged",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^app-/"
          ]
        }
      ]
    }
  },
  "stylintrc": {
    "reporterOptions": {
      "columns": [
        "lineData",
        "severity",
        "description",
        "rule"
      ],
      "columnSplitter": "  ",
      "showHeaders": false,
      "truncate": true
    },
    "blocks": false,
    "brackets": {
      "expect": "never",
      "error": true
    },
    "colons": {
      "expect": "never",
      "error": true
    },
    "commaSpace": {
      "expect": "always",
      "error": true
    },
    "commentSpace": {
      "expect": "always",
      "error": true
    },
    "cssLiteral": {
      "expect": "never",
      "error": true
    },
    "customProperties": [],
    "depthLimit": false,
    "duplicates": true,
    "efficient": {
      "expect": "always",
      "error": true
    },
    "exclude": [],
    "extendPref": false,
    "globalDupe": false,
    "groupOutputByFile": true,
    "indentPref": false,
    "leadingZero": {
      "expect": "never",
      "error": true
    },
    "maxErrors": false,
    "maxWarnings": false,
    "mixed": false,
    "mixins": [],
    "namingConvention": false,
    "namingConventionStrict": false,
    "none": {
      "expect": "never",
      "error": true
    },
    "parenSpace": false,
    "placeholders": {
      "expect": "always",
      "error": true
    },
    "prefixVarsWithDollar": {
      "expect": "always",
      "error": true
    },
    "quotePref": false,
    "semicolons": {
      "expect": "never",
      "error": true
    },
    "stackedProperties": {
      "expect": "never",
      "error": true
    },
    "trailingWhitespace": {
      "expect": "never",
      "error": true
    },
    "universal": false,
    "valid": true,
    "zeroUnits": {
      "expect": "never",
      "error": true
    },
    "zIndexNormalize": false
  }
}
