{
  "name": "angular-webshop",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server",
    "build": "npm run build:client",
    "build:all": "npm run clean:all && npm run build:client & npm run build:server",
    "build:all:aot": "npm run clean:all && npm run build:client:aot & npm run build:server:aot",
    "build:client": "./node_modules/webpack/bin/webpack.js --env.client -d && npm run fixdist",
    "build:client:aot": "./node_modules/webpack/bin/webpack.js --env.aot --env.client && npm run fixdist",
    "build:server": "./node_modules/webpack/bin/webpack.js --env.server -d && npm run clean:build",
    "build:server:aot": "ngc -p tsconfig.server.aot.json && ./node_modules/webpack/bin/webpack.js --env.aot --env.server && npm run clean:build",
    "watch:all": "npm run clean:all && npm run watch:client & npm run watch:server",
    "watch:client": "./node_modules/webpack/bin/webpack.js --env.client -d --watch",
    "watch:server": "./node_modules/webpack/bin/webpack.js --env.server -d --watch",
    "clean:all": "rimraf dist && rimraf src/aot",
    "clean:build": "rimraf dist/out-tsc && rimraf src/aot",
    "clean:dist": "rimraf dist",
    "fixdist": "./build.sh",
    "ngc:server": "ngc -p tsconfig.server.json",
    "server": "nodemon dist/main.server.bundle.js",
    "sw": "sw-precache --config=sw-precache-config.js",
    "test": "karma start ./karma.conf.js",
    "lint-staged": "lint-staged",
    "stylelint-staged": "stylelint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.0.2",
    "@angular/common": "4.0.2",
    "@angular/compiler": "4.0.2",
    "@angular/core": "4.0.2",
    "@angular/forms": "4.0.2",
    "@angular/http": "4.0.2",
    "@angular/platform-browser": "4.0.2",
    "@angular/platform-browser-dynamic": "4.0.2",
    "@angular/platform-server": "4.0.2",
    "@angular/router": "4.0.2",
    "angular2-jwt": "0.2.2",
    "base64-img": "1.0.3",
    "body-parser": "1.17.1",
    "compression": "1.6.2",
    "core-js": "2.4.1",
    "credential": "2.0.0",
    "express": "4.15.2",
    "fastclick": "1.0.6",
    "jsonwebtoken": "7.3.0",
    "mongodb": "2.2.25",
    "ng2-img-cropper": "0.8.6",
    "qrcode-generator": "1.2.0",
    "qrcodejs2": "0.0.2",
    "rxjs": "5.3.0",
    "speakeasy": "2.0.0",
    "sweetalert2": "6.6.0",
    "web-animations-js": "2.2.2",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.0.2",
    "@types/express": "4.0.35",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.12",
    "@types/speakeasy": "2.0.1",
    "autoprefixer": "6.7.7",
    "codelyzer": "3.0.0-beta.4",
    "css-loader": "0.28.0",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "json-loader": "0.5.4",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.1.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.3",
    "lint-staged": "3.4.0",
    "nodemon": "1.11.0",
    "postcss-loader": "1.3.3",
    "postcss-url": "6.0.4",
    "pre-commit": "1.2.2",
    "protractor": "5.1.1",
    "pug": "2.0.0-beta11",
    "pug-html-loader": "1.1.4",
    "purifycss-webpack": "0.6.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "script-loader": "0.7.0",
    "source-map-loader": "0.2.1",
    "style-loader": "0.16.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "stylus-loader": "3.0.1",
    "sw-precache": "5.1.0",
    "ts-node": "3.0.2",
    "tslint": "4.5.1",
    "typescript": "2.2.2",
    "url-loader": "0.5.8",
    "webpack-dev-server": "2.4.2"
  },
  "greenkeeper": {
    "branchPrefix": "dep-update-",
    "label": "dep-update",
    "ignore": [
      "tslint"
    ]
  },
  "lint-staged": {
    "*.ts": "tslint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint-staged",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^app-/"
          ]
        }
      ]
    }
  }
}
